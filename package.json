{
  "name": "@tylervigario/performance",
  "version": "1.0.0",
  "description": "ES Module for testing performance in Node CLI.",
  "main": "./lib/performance.mjs",
  "type": "module",
  "scripts": {
    "example": "node --experimental-modules ./example/index.mjs",
    "lint": "eslint --config .eslintrc.json --ext .mjs lib/ test/",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava --verbose",
    "test:coverage": "c8 ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeekLogic/performance.git"
  },
  "keywords": [
    "performance",
    "testing"
  ],
  "author": {
    "name": "Tyler Vigario",
    "email": "tylervigario90@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MeekLogic/performance/issues"
  },
  "homepage": "https://github.com/MeekLogic/performance#readme",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "c8": "^7.1.2",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0"
  },
  "dependencies": {},
  "ava": {
    "files": [
      "test/**/*"
    ],
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
